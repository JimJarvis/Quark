
def gcd(a, b)
{
    while a != 0:
    {
        c = a;
        % 'mod' should be a keyword
        a = b mod a;
        b = c;
    }
    return b;
}


% bitwise dot
% [] operator on an integer will extract the bit value at that location
def bit_dot(a, b)
{
    limit = 1;

    c = a & b;

    counter = 0;
    i = 0;

    while limit <= c:
    {
        counter += c[i++];
        limit <<= 1; % same as limit *= 2
    }

    return counter % 2;
}
